  <process id="smartClaim" name="smart claim" isExecutable="true">
    <startEvent id="startevent1" name="Start" activiti:initiator="${initiator.properties.userName}" activiti:formKey="dsw:creationdossierSinistre"></startEvent>
    <userTask id="usertask1" name="affecter dossier sinistre a un agent de saisie" activiti:assignee="${initiator.properties.userName}" activiti:formKey="dsw:affecterDossierSinistre"></userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="usertask1"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <userTask id="usertask2" name="Saisie de constat numérique" activiti:assignee="${dsw_agent}" activiti:formKey="dsw:saisieConstatNumerique">
      <extensionElements>
        <activiti:taskListener event="complete" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string><![CDATA[execution.setVariable("dsw_decisionConstatNumeriqueOutcome", task.getVariable("dsw_decisionConstatNumeriqueOutcome"));]]></activiti:string>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow2" sourceRef="usertask1" targetRef="usertask2"></sequenceFlow>
    <sequenceFlow id="flow3" sourceRef="usertask2" targetRef="exclusivegateway1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow4" name="dossier Invalide" sourceRef="exclusivegateway1" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${dsw_decisionConstatNumeriqueOutcome == 'invalid'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask3" name="valider constat" activiti:assignee="${dsw_assure}" activiti:formKey="dsw:validerConstatNumerique"></userTask>
    <sequenceFlow id="flow5" name="dossier complet" sourceRef="exclusivegateway1" targetRef="usertask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${dsw_decisionConstatNumeriqueOutcome == 'complet'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow6" sourceRef="usertask3" targetRef="endevent2"></sequenceFlow>
    <userTask id="usertask4" name="informations manquantes" activiti:assignee="${dsw_agent}" activiti:formKey="dsw:informationsManquantes"></userTask>
    <sequenceFlow id="flow7" name="dossier incomplet" sourceRef="exclusivegateway1" targetRef="usertask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${dsw_decisionConstatNumeriqueOutcome == 'incomplet'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow8" sourceRef="usertask4" targetRef="usertask5"></sequenceFlow>
    <userTask id="usertask5" name="informations complémentaires" activiti:assignee="${dsw_assure}" activiti:formKey="dsw:informationsComplémentaires"></userTask>
    <sequenceFlow id="flow9" sourceRef="usertask5" targetRef="usertask2"></sequenceFlow>
  </process>
